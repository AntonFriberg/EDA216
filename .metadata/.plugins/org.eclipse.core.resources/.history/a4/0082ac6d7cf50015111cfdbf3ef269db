import java.util.Date;

public class Bill {
	private int billId;
	private String custName,custAddr;
	private char isDel;
	private Date date;
	
	public Bill(){
	
	}
	
	public Bill(int billId, Date date, char isDel, String custName,String custAddr){
		this.billId=billId;
		this.date=date;
		this.isDel=isDel;
		this.custName=custName;
		this.custAddr=custAddr;
	}
}
